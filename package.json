{
  "name": "typerpc",
  "description": "Friendly Typescript Rpc Framework",
  "version": "0.0.1",
  "author": "Gary Becks",
  "bin": {
    "typerpc": "./bin/run"
  },
  "bugs": "https://github.com/getpoppn/typeRPC/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "fastify": "^3.0.2",
    "fastify-plugin": "^2.0.1",
    "fs-extra": "^9.0.1",
    "pino": "^6.4.0",
    "ts-morph": "^7.1.2",
    "tslib": "^1",
    "typescript-json-schema": "^0.42.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.1",
    "@types/mocha": "^5",
    "@types/node": "^14.0.23",
    "@types/pino": "^6.3.0",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/getpoppn/typeRPC",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "typerpc",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "getpoppn/typeRPC",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
