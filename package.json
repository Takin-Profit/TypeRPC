{
  "name": "@typerpc/cli",
  "description": "Cross Language Rpc Framework",
  "version": "1.0.1",
  "author": "Gary Becks techstar.dev@hotmail.com",
  "bin": {
    "typerpc": "./bin/run"
  },
  "bugs": "https://github.com/typerpc/cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-not-found": "^1.2.4",
    "await-to-js": "^2.1.1",
    "axios": "^0.19.2",
    "cli-ux": "^5.4.9",
    "fastify": "^3.1.1",
    "fastify-plugin": "^2.0.1",
    "fastify-sensible": "^3.0.1",
    "fs-extra": "^9.0.1",
    "listr": "^0.14.3",
    "nanoid": "^3.1.10",
    "ts-json-schema-generator": "^0.70.2",
    "ts-morph": "^7.1.2",
    "tslib": "^1",
    "@typerpc/types": "^1.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/cli-progress": "^3.7.0",
    "@types/fs-extra": "^9.0.1",
    "@types/listr": "^0.14.2",
    "@types/mocha": "^5",
    "@types/node": "^14.0.23",
    "@types/pino": "^6.3.0",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "pino": "^6.4.1",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.4.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/typerpc/cli",
  "keywords": [
    "typescript",
    "go",
    "rpc",
    "framework",
    "rest"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "typerpc",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ]
  },
  "repository": "typerpc/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
