{
  "name": "typerpc",
  "description": "High Perfomance Typescript Rpc Framework",
  "version": "0.0.2",
  "author": "Gary Becks",
  "bin": {
    "typerpc": "./bin/run"
  },
  "bugs": "https://github.com/getpoppn/typeRPC/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "fastify": "^3.1.1",
    "fastify-plugin": "^2.0.1",
    "fastify-sensible": "^3.0.1",
    "fs-extra": "^9.0.1",
    "@oclif/plugin-not-found": "^1.2.4",
    "ts-json-schema-generator": "^0.70.2",
    "ts-morph": "^7.1.2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.1",
    "@types/mocha": "^5",
    "@types/node": "^14.0.23",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/getpoppn/typeRPC",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "typerpc",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ]
  },
  "repository": "getpoppn/typeRPC",
  "scripts": {
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
