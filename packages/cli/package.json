{
  "name": "@typerpc/cli",
  "version": "0.1.1",
  "description": "typerpc command line tooling",
  "keywords": [
    "typerpc",
    "rpc",
    "json",
    "graphql",
    "typescript",
    "generator"
  ],
  "author": "Gary Becks <techstar.dev@hotmail.com>",
  "homepage": "https://typerpc.run",
  "license": "BSD-2-Clause",
  "bin": {
    "typerpc": "./bin/run"
  },
  "directories": {
    "lib": "src",
    "test": "tests"
  },
  "engines": {
    "node": ">=8.4.0"
  },
  "files": [
    "/bin",
    "/packages/cli/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./src/commands",
    "bin": "typerpc",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typerpc/typerpc.git"
  },
  "scripts": {
    "lint-fix": "eslint --fix src/**",
    "build": "yarn run clean && yarn run tsc",
    "clean": "rm -rf ./lib && rm -rf tsconfig.build.tsbuildinfo",
    "tsc": "tsc -b tscconfig.build.json",
    "postpack": "rm -f oclif.manifest.json",
    "posttest1": "eslint . --ext .ts --config .eslintrc",
    "prepack": "yarn run build && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "bugs": {
    "url": "https://github.com/typerpc/typerpc/issues"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/fs-extra": "^9.0.1",
    "@types/pino": "^6.3.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^11.0.1"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "@oclif/plugin-not-found": "^1.2.4",
    "@typerpc/config": "^0.1.1",
    "@typerpc/plugin": "^0.1.1",
    "@typerpc/plugin-manager": "^0.1.1",
    "@typerpc/schema": "^0.1.1",
    "fs-extra": "^9.0.1",
    "listr2": "^3.0.0",
    "pino": "^6.6.0",
    "ts-morph": "^8.1.0",
    "tslib": "^2.0.1"
  }
}
