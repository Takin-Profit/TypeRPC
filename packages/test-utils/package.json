{
  "name": "@typerpc/test-utils",
  "version": "0.1.1",
  "description": "typerpc test utilities",
  "keywords": [
    "typerpc"
  ],
  "author": "Gary Becks <techstar.dev@hotmail.com>",
  "homepage": "https://github.com/typerpc/typerpc",
  "license": "BSD-2-Clause",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "directories": {
    "lib": "src",
    "test": "tests"
  },
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typerpc/typerpc.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint-fix": "eslint --fix src/**",
    "build": "yarn run clean && yarn run tsc",
    "clean": "rm -rf ./dist && rm -rf tsconfig.build.tsbuildinfo",
    "tsc": "tsc -b tsconfig.build.json",
  },
  "bugs": {
    "url": "https://github.com/typerpc/typerpc/issues"
  },
  "dependencies": {
    "faker": "^5.1.0"
  },
  "devDependencies": {
    "@types/faker": "^4.1.12"
  }
}
